<h3 class="title">Inspectors</h3>

<div class="row">
  <div class="col-sm-12">
    <%= form_tag root_path, method: :get do %>
      <div class="row">
        <div class="col-sm-10">
          <div class="form-group">
            <div class="input-group">
              <%= label_tag :q, "Search for:", class: 'sr-only' %>
              <div class="input-group-addon"><span class="glyphicon glyphicon-search" aria-hidden="true"></span></div>
              <%= text_field_tag :q, nil, class: 'form-control', placeholder: 'Buscar inspectores...' %>
            </div>
          </div>
        </div>
        <div class="col-sm-2">
          <%= submit_tag "Buscar", class: 'btn btn-block btn-default' %>
        </div>
      </div>
    <% end %>
  </div>

  <% (1..4).each do %>
    <div class="col-sm-6 col-md-3">
      <div class="thumbnail">
        <%= image_tag "missing-image.gif" %>
        <div class="caption">
          <h4 class="title">Thumbnail label</h4>
        </div>
      </div>
    </div>
  <% end %>
</div>
