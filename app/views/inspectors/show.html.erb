<div class="row text-center">
  <h3 class="title texto-verde"><%= @inspector.name %></h3>
  <div style="border: 1px solid white;background-color: white;">
    <!-- <div class="col-md-6 col-md-offset-3"> -->
      <div class="thumbnail" style="margin-left:auto;margin-right:auto;width:305px;height:305px;">
        <%= image_tag @inspector.photo_url || "missing-image.gif" %>
      </div>
    <!-- </div> -->
    <table class="table">
      <tbody>
        <tr>
          <td class="texto-verde-wrap"><%= t('.vigencia') %></td>
          <td class ="bg-primary-negro"><%= @inspector.validity %></td>
        </tr>
        <tr>
          <td class="texto-verde-wrap"><%= t('.materia') %></td>
          <td class ="bg-primary-negro"><%= @inspector.matter %></td>
        </tr>
        <tr>
          <td class="texto-verde-wrap"><%= t('.supervisor') %></td>
          <td class ="bg-primary-negro"><%= @inspector.supervisor %></td>
        </tr>
        <tr>
          <td class="texto-verde-wrap"><%= t('.contacto') %></td>
          <td class ="bg-primary-negro"><%= @inspector.contact %></td>
        </tr>
    </table>
  </div>
</div>
