<%= form_for :post, :url => { :action => :quiere , :controller => :pages } do |f| %>
<p>
			<%= collection_select(:municipio, :municipio_id, Municipio.all, :id, :nombre, {:prompt => false})%>
</p>

<% @municipios.each_with_index do |municipio, i| %>
	<%=municipio.nombre%>|
<%end%>

<p>
	<aside class="call-to-action bg-primary">
		<div class="container">
			<div class="row">
				<div class="col-lg-12 text-center">
					<%= f.submit 'Continuar', :class => 'btn btn-lg btn-light' %>
				</div>
			</div>
		</div>
	</aside>
</p>
<% end %>