<%= form_for :post, :url => { :action => :quiere , :controller => :pages } do |f| %>
	<div class="container">
		<div class="row">
			<div class="col-lg-12 text-center">
				<div class="input-group">
					<div class="input-group-addon">
						<H3><%=f.label 'Ingresa tu municipio' %></H3>
						<BR>
						<%= text_field_tag :query, params[:query], class: "form-control", id: "result_search", autocomplete: "off" %><BR><BR>
						<% @municipios.each_with_index do |municipio, i| %>
							<%= municipio.nombre %>|
						<%end%>
					</div>
				</div>
				<br>
				<%= f.submit 'Continuar', :class => 'btn btn-lg btn-light' %>
			</div>
		</div>
	</div>
<% end %>
s





