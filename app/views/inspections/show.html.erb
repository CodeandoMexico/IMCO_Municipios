<div id="wrap">
  <div id="main" class="container clear-top">
    <div class="row row-padding-bottom">
      <div class="span12">
        <section id="services ">
          <div class="center-block-margin">
            <div class="container">
              <h4 class="texto-verde"><%= @inspection.name %></h4>
              <div style="border: 1px solid white;background-color:white;">
                <table class="table bg-primary-negro">
                  <tbody>
                    <tr>
                      <td class="texto-verde-wrap"><%= t('.subject') %></td>
                      <td><%= @inspection.matter %></td>
                    </tr>
                    <tr>
                      <td class="texto-verde-wrap"><%= t('.period') %></td>
                      <td><%= @inspection.duration %></td>
                    </tr>
                    <tr>
                      <td class="texto-verde-wrap"><%= t('.norm') %></td>
                      <td><%= @inspection.rule %></td>
                    </tr>
                    <tr>
                      <td class="texto-verde-wrap"><%= t('.before_tips') %></td>
                      <td><%= @inspection.before %></td>
                    </tr>
                    <tr>
                      <td class="texto-verde-wrap"><%= t('.during_tips') %></td>
                      <td><%= @inspection.during %></td>
                    </tr>
                    <tr>
                      <td class="texto-verde-wrap"><%= t('.after_tips') %></td>
                      <td><%= @inspection.after %></td>
                    </tr>
                    <tr>
                      <td class="texto-verde-wrap"><%= t('.sanctions_tips') %></td>
                      <td><%= @inspection.sanction %></td>
                    </tr>
                  </tbody>
                </table>
              </div>

              <br>
              <br>

              <div class="deck">
                <% if @inspection.requirements.any? %>
                  <h4 class='texto-verde'><%= t('.requisitos') %></h4>
                  <hr class="small">
                    <div class="row text-center ">
                      <div class="col-lg-12">
                        <%= render "shared/carrusel_inspections" %>
                      </div>
                    </div>
                <%end%>
              </div>

                


             

