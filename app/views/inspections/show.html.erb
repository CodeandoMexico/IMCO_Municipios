<h3 class="title"><%= @inspection.name %></h3>

<div style="border: 1px solid white;background-color: white;">
  <table class="table">
    <tbody>
      <tr>
        <td>Materia</td>
        <td><%= @inspection.subject %></td>
      </tr>
      <tr>
        <td>Duracion</td>
        <td><%= @inspection.period %></td>
      </tr>
      <tr>
        <td>Norma</td>
        <td><%= @inspection.norm %></td>
      </tr>
      <tr>
        <td>Antes</td>
        <td><%= @inspection.before_tips %></td>
      </tr>
      <tr>
        <td>Durante</td>
        <td><%= @inspection.during_tips %></td>
      </tr>
      <tr>
        <td>Despues</td>
        <td><%= @inspection.after_tips %></td>
      </tr>
      <tr>
        <td>Sancion</td>
        <td><%= @inspection.sanctions %></td>
      </tr>
    </tbody>
  </table>
</div>
