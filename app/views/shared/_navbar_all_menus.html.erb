<% if user_signed_in? %>
<nav class="navbar navbar-blanco navbar-fixed-top" role="navigation">
  <div class="container">
    <div class="navbar-header">
      <div class="dropdown nav navbar-nav navbar-left">
        <%unless @municipio.nil?%>
        <%=t('titles.titulo')%> | <a href="" id="dropdownMenu1" data-toggle="dropdown" aria-expanded="true" ><%=@municipio.nombre%><span class="caret m" ></span></a>
        <ul class="dropdown-menu" role="menu" aria-labelledby="dropdownMenu1">
          <%@municipios.each do |municipio|%>
          <%unless municipio == @municipio%>
          <li role="presentation"><%=link_to municipio.nombre,municipio_path(municipio), :role => "menuitem", :tabindex => "-1"%> </li>
          <%end%>
          <%end%>
        </ul>
        <%end%>
      </div>
    </div>
    <div id="navbar" class="nav nav-pills topnav">
      <ul class="nav navbar-nav navbar-right">
        <li> <m><%= link_to t('navbar.new'),municipio_formation_steps_path(@municipio)%></m></li>
        <li> <m> <%= link_to t('navbar.tramite'),municipio_procedure_lines_path(@municipio)%></m></li>
        <li> <m> <%= link_to t('navbar.inspections'),municipio_inspections_path(@municipio)%></m></li> 
        <li> <m> <%= link_to t('navbar.inspectors'),municipio_inspectors_path(@municipio)%></m></li>
        <li> | <m><%=link_to t('navbar.inicio'),root_path, class: "m" %></m></li>
      </ul>
    </div>
  </div>
</div>
</nav> 
<% else %>
<nav class="navbar navbar-default navbar-fixed-top" role="navigation">
  <div class="container">
    <div class="navbar-header">
      <span class="navbar-brand"><%=t('titles.titulo')%> </span>
    </div>
    <div id="navbar" class="nav nav-pills topnav">
      <ul class="nav navbar-nav navbar-right">
        <li> <m><%= link_to t('navbar.new'),municipio_formation_steps_path(@municipio)%></m></li>
        <li> <m> <%= link_to t('navbar.tramite'),municipio_procedure_lines_path(@municipio)%></m></li>
        <li> <m> <%= link_to t('navbar.inspections'),municipio_inspections_path(@municipio)%></m></li> 
        <li> <m> <%= link_to t('navbar.inspectors'),municipio_inspectors_path(@municipio)%></m></li>
        <li> | <m><%=link_to t('navbar.inicio'),root_path, class: "m" %></m></li>
      </ul>
    </div>
  </div>
 </div>
</nav>
<% end %>