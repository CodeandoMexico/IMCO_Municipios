<% if user_signed_in? %>
<nav class="navbar navbar-blanco navbar-fixed-top" role="navigation">
  <div class="container">
    <div class="navbar-header">
      <div class="dropdown nav navbar-nav navbar-left">
      <%unless @municipio.nil?%>
        MiNegocio | <a href="" id="dropdownMenu1" data-toggle="dropdown" aria-expanded="true" ><%=@municipio.nombre%><span class="caret m" ></span></a>
        <ul class="dropdown-menu" role="menu" aria-labelledby="dropdownMenu1">
          <%@municipios.each do |municipio|%>
          <%unless municipio == @municipio%>
            <li role="presentation"><%=link_to municipio.nombre,municipio_path(municipio), :role => "menuitem", :tabindex => "-1"%> </li>
          <%end%>
          <%end%>
        </ul>
        <%end%>
      </div>
    </div>
    <div id="navbar" class="nav nav-pills topnav">
      <ul class="nav navbar-nav navbar-right">
       <%= current_user.email %> | <m><%=link_to "Salir",root_path, class: "m" %></m>
     </ul>
   </div>
 </div>
</div>
</nav> 
<% else %>
<nav class="navbar navbar-default navbar-fixed-top" role="navigation">
  <div class="container">
    <div class="navbar-header">
      <span class="navbar-brand">MiNegocio</span>
    </div>
  </nav>
  <% end %>
