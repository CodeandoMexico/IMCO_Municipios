<a-verde > <%= render_breadcrumbs :separator => ' / ',:class => 'a-verde texto-verde' %></a-verde >
<div class="row text-center ">
  <%flash.each do |key, value| %>
  <div class="alert alert-danger"><%=value%></div>
  <% end %>
</div>
<div class="row text-center texto-verde">
  <%= content_tag :h1,t('.title') %> 
  <%if @line.nil? || @categoria.nil?%>
    <p><%=t('.no_procedure_lines_selected_subtitle')%></p>
    <p><%=t('.no_procedure_lines_selected_instruction')%></p>
  <%else @categoria.nil?%>
    <p><%=t('.no_procedure_lines_selected_resultado')%> <%=" #{Line.find(@line).name} para persona #{tipo(@tipo)}, #{@categoria}."%> </p>
  <%end%>
  <!-- <hr class="small"> -->
</div>
<%= form_for :get, :html => {:method => :get}, :url => { :controller => :procedure_lines, :action => :index }  do |f| %>
  <div class="col-md-8 col-md-offset-1 text-center texto-verde-wrap">
  <div class="row text-left">
    <div class="row">
      <div class="col-xs-6 col-sm-3 texto-margin-top text-right">
        <%=t('.procedure_lines_select_name')%>
      </div>
      <div class="clearfix visible-xs-block">
      </div>
      <div class="col-md-offset-3">
        <%= f.select :lines, Line.where(city_id: @city).map { |u| [u.name, u.id] },{ include_blank: true ,selected: @line }, { class: 'chosen-select' }%>
      </div>
    </div>
    <br>
    <div class="row">
      <div class="col-xs-6 col-sm-3 texto-margin-top text-right">
        <%= t('.procedure_lines_radio_name')%>
      </div>
      <div class="clearfix visible-xs-block"></div>
      <div class="col-md-offset-3">
        <div class="btn-group btn-toggle" data-toggle="buttons">
          <%if @tipo == 'TF'%>
          <label class= "btn btn-default active"><%= t('.radio_fisica')%> <%= radio_button_tag :rating, 'TF',true%></label>
          <label class= "btn btn-default"><%= t('.radio_moral')%> <%= radio_button_tag :rating, 'TM'%></label>
          <label class= "btn btn-default"><%= t('.radio_ambas')%> <%= radio_button_tag :rating, 'A'%></label>
          <%elsif @tipo == 'TM'%>
          <label class= "btn btn-default "><%= t('.radio_fisica')%> <%= radio_button_tag :rating, 'TF'%></label>
          <label class= "btn btn-default active"><%= t('.radio_moral')%> <%= radio_button_tag :rating, 'TM',true%></label>
          <label class= "btn btn-default"><%= t('.radio_ambas')%> <%= radio_button_tag :rating, 'A'%></label>
          <%else%>
          <label class= "btn btn-default "><%= t('.radio_fisica')%> <%= radio_button_tag :rating, 'TF'%></label>
          <label class= "btn btn-default "><%= t('.radio_moral')%> <%= radio_button_tag :rating, 'TM'%></label>
          <label class= "btn btn-default active"><%= t('.radio_ambas')%> <%= radio_button_tag :rating, 'A',true%></label>
          <%end%>
        </div>
      </div>
    </div>
    <br>
