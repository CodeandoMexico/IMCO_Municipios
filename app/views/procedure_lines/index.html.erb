          <%end%>
        </div>
      </div>
    </div>
    <br>
<div class="row text-lef">
        <div class="actions col-md-offset-3 text-lef">
          <div class="col-md-6 margin-bottom-button"><%= f.submit t('buttons.buscar'),:class => 'btn btn-success btn-lg outlineverde btn-block'%></div>
        </div>
